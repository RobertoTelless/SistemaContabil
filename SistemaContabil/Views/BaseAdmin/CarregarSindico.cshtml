                                    @Html.ButtonLinkIndex(text: "Ver Todos", action: "MontarTelaControleVeiculo", controller: "ControleVeiculo", buttonClass: "white")
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
