                                                    {
                                                        <td>@cond.COVE_DT_SAIDA.Value.ToShortDateString() @cond.COVE_DT_SAIDA.Value.ToShortTimeString()</td>
                                                    }
                                                    else
                                                    {
                                                        <td>-</td>
                                                    }
                                                    <td>@cond.COVE_NM_PLACA</td>
                                                </tr>
                                            }
                                        </tbody>
                                    </table>
                                }
