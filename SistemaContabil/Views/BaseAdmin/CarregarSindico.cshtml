            <div class="row">
                <div class="col-lg-12">
                    <div class="ibox float-e-margins">
                        <div class="ibox-title">
                            <span class="label label-primary pull-right">Hoje</span>
                            <h5><a href="@Url.Action("MontarTelaES", "EntradaSaida")">Entradas/Saídas de Visitantes</a></h5>
                        </div>
                        <div class="ibox-content">
                            <small>Entradas/Saídas Hoje</small>
                            <h1 class="no-margins">@ViewBag.NumES</h1>
                            <hr />
                            <small>Últimas Entradas/Saídas</small>
                            <br />
                            <br />
                            <div class="row">
                                @if ((ViewBag.ListaES as List<EntitiesServices.Model.ENTRADA_SAIDA>).Count > 0)
                                {
                                    <table class="table table-striped table-bordered table-hover">
                                        <thead style="background-color:lightsteelblue">
                                            <tr>
                                                <th>Unidade</th>
                                                <th>Entrada</th>
                                                <th>Saída</th>
                                                <th>Nome</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            @foreach (var cond in (ViewBag.ListaES as List<EntitiesServices.Model.ENTRADA_SAIDA>).OrderByDescending(p => p.ENSA_DT_ENTRADA).Take(3))
                                            {
                                                <tr>
                                                    <td>@cond.UNIDADE.UNID_NR_NUMERO</td>
                                                    <td>@cond.ENSA_DT_ENTRADA.Value.ToShortDateString() @cond.ENSA_DT_ENTRADA.Value.ToShortTimeString()</td>
                                                    @if (cond.ENSA_DT_SAIDA !=  null)
                                                    {
                                                        <td>@cond.ENSA_DT_SAIDA.Value.ToShortDateString() @cond.ENSA_DT_SAIDA.Value.ToShortTimeString()</td>
                                                    }
                                                    else
                                                    {
                                                        <td>-</td>
                                                    }
                                                    @if (cond.ENSA_NM_NOME != null)
                                                    {
                                                        <td>@cond.ENSA_NM_NOME.Substring(0, cond.ENSA_NM_NOME.IndexOf(" "))</td>
                                                    }
                                                    else
                                                    {
                                                        <td>Sem Identificação</td>
                                                    }
                                                </tr>
                                            }
                                        </tbody>
                                    </table>
                                }
                                <div class="form-group">
                                    @Html.ButtonLinkIndex(text: "Ver Todos", action: "MontarTelaES", controller: "EntradaSaida", buttonClass: "white")
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-12">
