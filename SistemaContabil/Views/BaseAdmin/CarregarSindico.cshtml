                                            @foreach (var cond in (ViewBag.ListaCV as List<EntitiesServices.Model.CONTROLE_VEICULO>))
                                            {
                                                <tr>
                                                    @if (cond.UNIDADE != null)
	                                                {
                                                        <td>@cond.UNIDADE.UNID_NR_NUMERO</td>
                                                    }
                                                    else
                                                    {
                                                        <td>-</td>
                                                    }
